<section class="StackCard">
    <img class="img-fluid" alt=@ImgAlt src=@ImgSrc />
    <footer>@CardTitle</footer>
</section>

@code {

    [Parameter]
    public string ImgAlt { get; set; } = "";

    [Parameter]
    public string ImgSrc { get; set; } = "/img/profile-img.jpg";

    [Parameter]
    public string CardCode { get; set; } = "0";

    [Parameter]
    public string CardTitle { get; set; } = "Oscar";
}
