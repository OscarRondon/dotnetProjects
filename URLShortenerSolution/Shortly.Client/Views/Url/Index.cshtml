@model List<Url>;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@

@{
    var currentDateTime = DateTime.UtcNow;
    var allUrls = ViewData["AllUrls"] as List<string>;
}

<h4>Example</h4>
<p>Original links using Model</p>

@foreach (var link in @Model)
{
    <p>Original link: @link.OriginalLink</p>
}

<p>Original link: @ViewData["ShortenedUrl"]</p>
@foreach (var link in allUrls)
{
    <p>Original DataView link: @link</p>
}

<p>Original link: @ViewBag.ShortenedUrlVB</p>
@foreach (var link in ViewBag.AllUrlsVB)
{
    <p>Original DataView link: @link</p>
}

@if (TempData["SuccessMessage"] != null)
{
    <p>@TempData["SuccessMessage"]</p>
}